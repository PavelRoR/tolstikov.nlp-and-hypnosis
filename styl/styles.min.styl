	// 	position: absolute
	// 	left -5%
	// 	right 0
	// 	top 3px
	// 	width 110%
	// 	height 100%
	// 	border: 1px dashed #0a1938
	&-2
			border-left: 0
			border-right: 0
			width 100%
			left 0
		// &:after
.white-container
	background-color: rgb(255, 255, 255);
	box-shadow: 0px 0px 9.4px 0.6px rgba(24, 41, 70, 0.3);
	width 100%
	padding 30px
	&-last
		background-color: #F9FAFA
.general-list
	li
		list-style: none
		position relative
		padding 6px 0 5px 50px
		min-height 41px
		margin-bottom: 10px
		&:last-child
			margin-bottom: 0
		&:before
			content: ''
			display block
			position: absolute
			top 0
			left 0
			background-color: #001236
			border-radius: 50px
			padding 4px
			border: 1px dashed #001236
			width 40px
			height 40px
			background-clip: content-box
		&:after
			content: ''
			display block
			position: absolute
			width 20px
			height 9px
			transform: rotate(-50deg)
			border-left: 2px solid #DCE0E2
			border-bottom: 2px solid #DCE0E2
			top: 14px;
			left: 10px;
		&:nth-child(2n+1)
			&:before
				background-color: #EC4A00
	&-2
		li:before
			background-color: #EC4A00
		li:nth-child(2n + 1):before
			background-color: #001236
	&-3
		li
			position relative
			list-style: none
			line-height: 24px
			padding-left: 34px
			&:before
				content: ''
				display block
				position: absolute
				left 0
				top 9px
				border-radius: 50px
				width 9px
				height 9px
				background-color: #282016
.input
	display block
	background-color: rgb(255, 255, 255);
	width 100%
	max-width: 250px;
	padding 0 10px
	color #2a2d3e
	height: 36px;
	border 0
	line-height: 18px
	border 1px solid #0a1938
@-webkit-keyframes autofill
	to
		color: #2a2d3e
		background-color: #fff
input:-webkit-autofill
	-webkit-animation-name: autofill;
	-webkit-animation-fill-mode: both;
.alert-message {
	clear both
	display: none;
	font-size: 16px;
	padding-top: 10px;
	line-height: 16px
	color: #f00
	text-align: center
}
.checking {
	display: table;
	width 100%
	max-width 477px
	margin: 5px auto 0
	text-align: left
}

.checking .check-label {
	line-height: 27px;
	cursor: pointer;
	position: relative;
	display: table-cell;
	vertical-align: middle;
	padding: 0 0 0 25px;
	text-align: left;
	font-size: 14px
	color #0a1938
}

.checking .check-label a
	text-decoration: underline
	display: inline
	color #0a1938
	&:focus
		text-decoration: underline
		color #0a1938
	&:hover
		text-decoration: none
		color #0a1938

.checking .check-label:before {
	content: '';
	background-color: red
	position: absolute;
	left: 0;
	top: 3px;
	width: 20px;
	height: 20px;
	border 1px dashed #0a1938
	border-radius: 0
}

.checking .check-label:after {
	content: '';
	position: absolute;
	left: 8px;
	top: -10px;
	width: 6px;
	height: 11px;
	background-color: transparent;
	opacity: 0;
	border-bottom: 2px solid #0a1938
	border-right: 2px solid #0a1938
	-webkit-transform: rotate(41deg);
	-ms-transform: rotate(41deg);
	transform: rotate(41deg);
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;

}

.check {
	position: absolute;
	z-index: -1;
	opacity: 0;
	width: 0;
	height 0;
	border: 0;
	left -99999px;
}

.check:checked + .check-label:before {
	background-color: transparent

}

.check:checked + .check-label:after {
	opacity: 1;
	top: 7px;
	-webkit-transition: all 0.5s ease;
	-o-transition: all 0.5s ease;
	transition: all 0.5s ease;
}


