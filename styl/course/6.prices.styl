#prices
	padding 50px 0
// 	@extend #for
// .general-title-prices
// 	@extend .general-title-hypnosis
.packet
	width 100%
	max-width 380px
	margin 0 auto
	background-color:	#fff;
	box-shadow: 0px 0px 9.4px 0.6px	rgba(24, 41, 70, .3);
	padding: 20px;
	&-head-title
		text-align: center
		font-size: 22px
		font-family: RO
		color: #F0651B
		line-height: 1.4
		width 118%
		left -9%
		margin 0 0 20px
		position relative
		border: 1px dashed #0a1938
	&-body
		padding-bottom: 20px
		min-height 298px
.general-list-packet
	li
		padding-left: 19px
		margin-bottom: 10px
		&:last-child
			margin-bottom: 0
.more-button
	font-size: 14px
	display: inline-block
	border-bottom: 1px dashed
	color #aaa
	margin-bottom: 10px
	cursor pointer
.cost
	text-align: center
	font-size: 22px
	margin-bottom: 15px
	font-family: OSB
	span
		display block
		font-family: RO
		font-size: 32px
	&-full
		span
			text-decoration: line-through
		.unbroken
			text-decoration: none
	&-sale
		color red
	&-today
		color green
.prepayment
	text-align: center
	&-title
		color red
		font-size: 36px
		font-family: RO
		margin 30px 0 0
	&-text
		// color #fff
		font-size: 20px
		margin 10px 0 25px
.timer-title
	font-size: 36px
	color red
	font-family: RO
	margin 40px 0 0
	text-align: center
.alert-danger
	color:#a94442;
	background-color:#f2dede;
	border:1px solid #ebccd1;
	text-align: center
	padding: 15px;
	width 100%
	max-width 450px
	margin 20px auto 0
	font-size: 22px
	border-radius: 3px
.flip-clock-wrapper
	width: 100%
	max-width 460px
	margin-top: 20px
.flip-clock-divider .flip-clock-label
	color #000
.flip-clock-dot
	background-color: red
.flip-clock-wrapper ul li a div div.inn
	color: #fff
	background-color: red
	text-shadow: 2px 2px 2px rgba(#022a37,.6);